<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.enation.app.javashop.mapper.member.ShipTemplateMapper">
    <!-- 根据店铺id获取店铺运费模板集合 -->
    <select id="selectShipTemplateList" resultType="com.enation.app.javashop.model.shop.vo.ShipTemplateSellerVO" useCache="false">
        select * from es_ship_template where seller_id = #{seller_id}
    </select>

</mapper>
